    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "cookie": "^0.4.1",
    "json-schema": "^0.4.0",
    "prisma": "^3.14.0",
    "svelte-i18n": "^3.4.0"
  }
